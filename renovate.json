{
    "extends": ["config:base"],
    "regexManagers": [
      {
        "fileMatch": ["^config\\.tm\\.hcl$"],
        "matchStrings": [
          "version\\s*=\\s*\"(?<currentValue>[^\"]+)\"",
          "source\\s*=\\s*\"(?<datasource>hashicorp/.+)\""
        ],
        "datasourceTemplate": "terraform-provider",
        "versioningTemplate": "hashicorp"
      }
    ]
  }
  